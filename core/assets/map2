try {
            Scanner level = new Scanner(new FileInputStream("map1"));
            while(level.hasNextLine())
            {
                String line = level.nextLine();
                //x=y=z=0;
                while(x<line.length())
                {
                   // System.out.print(line.charAt(x));
                    world[x][y][z]=(int)line.charAt(x)-48;
                    //System.out.println(x+"  "+y+"   "+z+"   "+ world[x][y][z]);
                    x++;
                }
                worldX=x;
                x=0;
                //System.out.println();
                y++;
                worldY=y;
                worldZ=z;
            }
            level.close();
                try {
                    z=1;
                    x=y=0;
                level = new Scanner(new FileInputStream("map2"));
                while(level.hasNextLine())
                {
                    String line = level.nextLine();
                    
                    while(x<line.length())
                    {
                        //System.out.print(line.charAt(x));
                        world[x][y][z]=(int)line.charAt(x)-48;//line.charAt(x);
                        //System.out.println(x+"  "+y+"   "+z+"   "+ world[x][y][z]);
                        x++;
                    }
                    //System.out.println();
                    x=0;
                    y++;
                }
                z++; //delete maybe?
                for(z=z; z<10; z++)
                {
                    for(int i=0; i<5; i++)
                    {
                        for(int j=0; j<5; j++)
                        {
                            //System.out.println("");
                            world[i][j][z]=3;
                            //System.out.println(i+"  "+j+"   "+z+"   "+ world[i][j][z]);
                        }
                    }
                }
                //z++;
                worldZ=z;
                level.close();
            } catch (FileNotFoundException ex) 
            {

            }
        } catch (FileNotFoundException ex) 
