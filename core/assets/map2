//            }
            x+=(ax+.1)*.9;
            y+=ay+1.05;
            z+=az*.9;//(az* (106.0/100.0));
            tempZ=z;
            if(tempZ%1>0 && tempZ<1)
            {
                tempZ++;
                tempZ=(int)tempZ;
            }
        }
        else if(facing.equals("WEST"))
        { 
            x+=(ax+.1)*.9;
            y+=ay+1.05;
            z+=az*.9;//(az* (106.0/100.0));
            tempZ=z;
            if(tempZ%1>0 && tempZ<1)
            {
                tempZ++;
                tempZ=(int)tempZ;
            }
        }
        else if(facing.equals("SOUTH"))
        { 
            x+=(ax+.1)*.9;
            y+=ay+1;
            z+=az*.9;//(az* (106.0/100.0));
            tempZ=z;
            if(tempZ%1>0 && tempZ<1)
            {
                tempZ++;
                tempZ=(int)tempZ;
            }
