if(facing.equals("NORTH"))
        { 
            //x+=(ax+.1)*.9;
            x+=ax;
            y+=ay;
            z+=az;//*.9;//(az* (106.0/100.0));
            tempZ=z;
            if(tempZ%1>0 && tempZ<1)
            {
                tempZ++;
                tempZ=(int)tempZ;
            }

            
        }
        else if(facing.equals("EAST"))
        { 
//            x+=ax*.97;
//            y+=ay+1.1;
//            z+=az*.9;
//            tempZ=z;
